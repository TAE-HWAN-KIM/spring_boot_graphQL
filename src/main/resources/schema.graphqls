type Query {
  bookById(id: String): Book 
  books: [Book]
  users:[User]
}

type Book {
  id: String
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: String
  firstName: String
  lastName: String
}

type User{
  userId: Int!
  name: String
}

type Mutation {
 createUser(title: String!): User
 updateUser(id: Int!, title: String!): User
}